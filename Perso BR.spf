    For i = 1 To NRow
        IdInterloc = MyRcs.GetValue(1)
        rSql="SELECT " &amp; FonctionInterlocComm &amp; " FROM FONCTIONINTERLOC WHERE ID='" &amp; IdInterloc &amp; "'"
        set TestRcs = CreateRecordset(rSql &amp; "/* SELECT : mention obligatoire */")
